{
    "OpenUIOnStartup": false,
    "WebApiExecutablePath":"./../../../../../ScreenTimeTracker.WebApi/bin/Debug/net10.0/ScreenTimeTracker.WebApi.exe",
    "WorkerServiceExecutablePath":"./../../../../../ScreenTimeTracker.WorkerService/bin/Debug/net10.0/ScreenTimeTracker.WorkerService.exe",
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "./Logs/Tray/log-.txt",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7
                }
            }
        ]
    }
}