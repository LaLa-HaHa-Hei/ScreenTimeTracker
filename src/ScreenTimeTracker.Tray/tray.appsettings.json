{
    "DefaultUIUrl": "http://localhost:5124",
    "OpenUIOnStartup": false,
    "WebApiExecutablePath":"./ScreenTimeTracker.WebApi.exe",
    "WorkerServiceExecutablePath":"./ScreenTimeTracker.WorkerService.exe",
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext"
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "./Logs/Tray/log-.txt",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7
                }
            }
        ]
    }
}